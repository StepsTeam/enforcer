{
  "bash": {
    "extensions": ["sh", "bash"],
    "heuristics": ["#!/bin/bash", "echo ", "fi", "then", "source "],
    "repository_url": "https://github.com/tree-sitter/tree-sitter-bash",
    "traintrack": "yes"
  },
  "json": {
    "extensions": ["json"],
    "heuristics": ["{", "["],
    "repository_url": "https://github.com/tree-sitter/tree-sitter-json",
    "traintrack": "no"
  },
  "php": {
    "extensions": ["php", "php3", "php4", "php5", "phtml", "inc"],
    "heuristics": ["<?php"],
    "repository_url": "https://github.com/tree-sitter/tree-sitter-php",
    "traintrack": "yes"
  },
  "rust": {
    "extensions": ["rs"],
    "heuristics": ["fn ", "let mut ", "mod ", "pub "],
    "repository_url": "https://github.com/tree-sitter/tree-sitter-rust",
    "traintrack": "yes"
  },
  "yaml": {
    "extensions": ["yaml", "yml"],
    "heuristics": ["---"],
    "repository_url": "https://github.com/ikatyang/tree-sitter-yaml",
    "traintrack": "no"
  }
}
